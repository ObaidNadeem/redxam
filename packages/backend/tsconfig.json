{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2019",
    "lib": [
      "ESNext"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "paths": {
      "blockchair-api": [
        "apis/blockchair"
      ],
      "@/*": [
        "*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
